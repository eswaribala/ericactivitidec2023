package com.cts.sbactivitiapp.services;

import org.activiti.api.process.model.IntegrationContext;
import org.activiti.api.process.runtime.connector.Connector;
import org.slf4j.Logger;
import org.slf4j.LoggerFactory;
import org.springframework.stereotype.Service;

@Service(value="loginservice")
public class LoanService implements Connector{
	private Logger logger = LoggerFactory.getLogger(LoanService.class);

	 public IntegrationContext execute(IntegrationContext integrationContext) {
	 logger.info("Some service task logic... [processInstanceId=" + integrationContext.getProcessInstanceId() + "]");

	 return integrationContext;
	 }

	@Override
	public IntegrationContext apply(IntegrationContext t) {
		 logger.info("Some service task logic... [processInstanceId=" + integrationContext.getProcessInstanceId() + "]");

		 return integrationContext;
	}

}
